{
  "name": "sinbix",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "publish:packages-common-utils": "npx nx build packages-common-utils && cd dist/packages/common/utils && npm publish --access=public",
    "publish:sinbix-sinbix": "npx nx build sinbix-sinbix && cd dist/packages/sinbix/sinbix && npm publish --access=public",
    "publish:sinbix-cli": "npx nx build sinbix-cli && cd dist/packages/sinbix/cli && npm publish --access=public",
    "publish:sinbix-core": "npx nx build sinbix-core && cd dist/packages/sinbix/core && npm publish --access=public",
    "publish:sinbix-utils": "npx nx build sinbix-utils && cd dist/packages/sinbix/utils && npm publish --access=public",
    "publish:plugins-common": "npx nx build plugins-common && cd dist/packages/plugins/common && npm publish --access=public",
    "publish:plugins-node": "npx nx build plugins-node && cd dist/packages/plugins/node && npm publish --access=public",
    "publish:plugins-plugin": "npx nx build plugins-plugin && cd dist/packages/plugins/plugin && npm publish --access=public",
    "publish:common": "npm run publish:packages-common-utils",
    "publish:sinbix": "npm run publish:sinbix-core && npm run publish:sinbix-utils && npm run publish:sinbix-cli && npm run publish:sinbix-sinbix",
    "publish:plugins": "npm run publish:plugins-common && npm run publish:plugins-node && npm run publish:plugins-plugin",
    "publish:all": "npm run publish:common && npm run publish:sinbix && npm run publish:plugins",
    "install:core": "npm i --save-dev @sinbix/core@latest @sinbix/common@latest @sinbix/cli@latest",
    "update:core": "npm run publish:all && npm run install:core",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:docker": "cd packages/demo/deploy && docker-compose up -d",
    "demo-auth:make-migration": "npm run demo-auth:migrate && npx ts-node -P packages/demo/apps/nest/server-auth-ms/db/tsconfig.json node_modules/.bin/typeorm migration:generate --config packages/demo/apps/nest/server-auth-ms/db/src/auth/auth.ormconfig.ts -d packages/demo/apps/nest/server-auth-ms/db/src/auth/migrations -n",
    "demo-auth:migrate": "npx ts-node -P packages/demo/apps/nest/server-auth-ms/db/tsconfig.json node_modules/.bin/typeorm migration:run -t=false --config packages/demo/apps/nest/server-auth-ms/db/src/auth/auth.ormconfig.ts",
    "demo-auth:revert": "npx ts-node -P packages/demo/apps/nest/server-auth-ms/db/tsconfig.json node_modules/.bin/typeorm migration:revert --config packages/demo/apps/nest/server-auth-ms/db/src/auth/auth.ormconfig.ts",
    "demo-auth:migration": "npm run demo-auth:make-migration && npm run demo-auth:migrate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.1.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@capacitor/core": "^2.4.7",
    "@datorama/akita": "^6.0.1",
    "@datorama/akita-ng-entity-service": "^6.0.0",
    "@datorama/akita-ng-router-store": "^6.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.9.11",
    "@nestjs/microservices": "^7.6.14",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.21.1",
    "d3": "^6.3.1",
    "dagre-d3": "^0.6.4",
    "document-register-element": "1.13.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "lodash": "^4.17.20",
    "material-design-icons": "^3.0.1",
    "mongodb": "^3.6.4",
    "nats": "^1.4.12",
    "normalize.css": "^8.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "tippy.js": "^6.2.7",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.31",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
