{
  "version": 1,
  "projects": {
    "sinbix": {
      "root": "packages/sinbix",
      "sourceRoot": "packages/sinbix/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/sinbix/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/sinbix/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/sinbix",
            "tsConfig": "packages/sinbix/tsconfig.lib.json",
            "packageJson": "packages/sinbix/package.json",
            "main": "packages/sinbix/src/index.ts",
            "assets": [
              "packages/sinbix/*.md"
            ]
          }
        }
      }
    },
    "common-e2e": {
      "projectType": "application",
      "root": "packages/common-e2e",
      "sourceRoot": "packages/common-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "plugins-common:build",
            "npmPackageName": "@sinbix/common",
            "pluginOutputPath": "dist/packages/common",
            "jestConfig": "packages/common-e2e/jest.config.js"
          }
        }
      }
    },
    "cli": {
      "root": "packages/cli",
      "sourceRoot": "packages/cli/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/cli/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cli/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/cli",
            "tsConfig": "packages/cli/tsconfig.lib.json",
            "packageJson": "packages/cli/package.json",
            "main": "packages/cli/src/index.ts",
            "assets": [
              "packages/cli/*.md"
            ]
          }
        }
      }
    },
    "dep-graph": {
      "root": "packages/dep-graph",
      "sourceRoot": "packages/dep-graph/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/packages/dep-graph",
            "index": "packages/dep-graph/src/index.html",
            "main": "packages/dep-graph/src/main.ts",
            "polyfills": "packages/dep-graph/src/polyfills.ts",
            "tsConfig": "packages/dep-graph/tsconfig.app.json",
            "assets": [
              "packages/dep-graph/src/favicon.ico",
              "packages/dep-graph/src/assets"
            ],
            "styles": [
              "packages/dep-graph/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/dep-graph/src/main.ts",
                  "with": "packages/dep-graph/src/main.release.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "dep-graph:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "dep-graph:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/dep-graph/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/dep-graph/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dep-graph-e2e": {
      "root": "packages/dep-graph-e2e",
      "sourceRoot": "packages/dep-graph-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/dep-graph-e2e/cypress.json",
            "tsConfig": "packages/dep-graph-e2e/tsconfig.e2e.json",
            "devServerTarget": "dep-graph:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dep-graph:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/dep-graph-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/core/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/src/index.ts",
            "assets": [
              "packages/core/*.md"
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages/core"
          ],
          "options": {
            "commands": [
              {
                "command": "nx build-base core"
              },
              {
                "command": "nx build dep-graph --configuration production"
              },
              {
                "command": "node ./tools/scripts/copy-dep-graph.js"
              }
            ],
            "parallel": false
          }
        }
      }
    },
    "node-e2e": {
      "projectType": "application",
      "root": "packages/node-e2e",
      "sourceRoot": "packages/node-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "node:build",
            "npmPackageName": "@sinbix/node",
            "pluginOutputPath": "dist/packages/node",
            "jestConfig": "packages/node-e2e/jest.config.js"
          }
        }
      }
    },
    "node": {
      "root": "packages/node",
      "sourceRoot": "packages/node/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/node/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/node/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/node",
            "tsConfig": "packages/node/tsconfig.lib.json",
            "packageJson": "packages/node/package.json",
            "main": "packages/node/src/index.ts",
            "assets": [
              "packages/node/*.md",
              {
                "input": "./packages/node/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/node",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/node",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages/node"
          ],
          "options": {
            "commands": [
              {
                "command": "nx build-base node"
              }
            ],
            "parallel": false
          }
        }
      }
    },
    "plugin": {
      "root": "packages/plugin",
      "sourceRoot": "packages/plugin/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/plugin/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/plugin/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/plugin",
            "tsConfig": "packages/plugin/tsconfig.lib.json",
            "packageJson": "packages/plugin/package.json",
            "main": "packages/plugin/src/index.ts",
            "assets": [
              "packages/plugin/*.md",
              {
                "input": "./packages/plugin/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/plugin",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/plugin",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages/plugin"
          ],
          "options": {
            "commands": [
              {
                "command": "nx build-base plugin"
              }
            ],
            "parallel": false
          }
        }
      }
    },
    "plugin-e2e": {
      "projectType": "application",
      "root": "packages/plugin-e2e",
      "sourceRoot": "packages/plugin-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "plugin:build",
            "npmPackageName": "@sinbix/plugin",
            "pluginOutputPath": "dist/packages/plugin",
            "jestConfig": "packages/plugin-e2e/jest.config.js"
          }
        }
      }
    },
    "plugins-common": {
      "root": "packages/plugins/common",
      "sourceRoot": "packages/plugins/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/plugins/common/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/plugins/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/plugins/common",
            "tsConfig": "packages/plugins/common/tsconfig.lib.json",
            "packageJson": "packages/plugins/common/package.json",
            "main": "packages/plugins/common/src/index.ts",
            "assets": [
              "packages/plugins/common/*.md",
              {
                "input": "./packages/plugins/common/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/plugins/common",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/plugins/common",
                "glob": "builders.json",
                "output": "."
              },
              {
                "input": "./packages/plugins/common",
                "glob": "migrations.json",
                "output": "."
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages/plugins/common"
          ],
          "options": {
            "commands": [
              {
                "command": "nx build-base plugins-common"
              }
            ],
            "parallel": false
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "dep-graph"
}
