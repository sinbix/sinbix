{
  "version": 1,
  "projects": {
    "sinbix": {
      "root": "packages/sinbix",
      "sourceRoot": "packages/sinbix/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/sinbix/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/sinbix/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/sinbix",
            "tsConfig": "packages/sinbix/tsconfig.lib.json",
            "packageJson": "packages/sinbix/package.json",
            "main": "packages/sinbix/src/index.ts",
            "assets": ["packages/sinbix/*.md"]
          }
        }
      }
    },
    "devkit": {
      "root": "packages/devkit",
      "sourceRoot": "packages/devkit/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/devkit/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/devkit/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/devkit",
            "tsConfig": "packages/devkit/tsconfig.lib.json",
            "packageJson": "packages/devkit/package.json",
            "main": "packages/devkit/src/index.ts",
            "assets": [
              "packages/devkit/*.md",
              {
                "input": "./packages/devkit/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/devkit",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/devkit",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "devkit-e2e": {
      "projectType": "application",
      "root": "packages/devkit-e2e",
      "sourceRoot": "packages/devkit-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "devkit:build",
            "npmPackageName": "@sinbix/devkit",
            "pluginOutputPath": "dist/packages/devkit",
            "jestConfig": "packages/devkit-e2e/jest.config.js"
          }
        }
      }
    },
    "common": {
      "root": "packages/common",
      "sourceRoot": "packages/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/common/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/common",
            "tsConfig": "packages/common/tsconfig.lib.json",
            "packageJson": "packages/common/package.json",
            "main": "packages/common/src/index.ts",
            "assets": ["packages/common/*.md"]
          }
        }
      }
    },
    "cli": {
      "root": "packages/cli",
      "sourceRoot": "packages/cli/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/cli/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cli/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/cli",
            "tsConfig": "packages/cli/tsconfig.lib.json",
            "packageJson": "packages/cli/package.json",
            "main": "packages/cli/src/index.ts",
            "assets": ["packages/cli/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
