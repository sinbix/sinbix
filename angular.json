{
  "version": 1,
  "projects": {
    "sinbix": {
      "root": "packages/sinbix",
      "sourceRoot": "packages/sinbix/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/sinbix/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/sinbix/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/sinbix",
            "tsConfig": "packages/sinbix/tsconfig.lib.json",
            "packageJson": "packages/sinbix/package.json",
            "main": "packages/sinbix/src/index.ts",
            "assets": ["packages/sinbix/*.md"]
          }
        }
      }
    },
    "common": {
      "root": "packages/common",
      "sourceRoot": "packages/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/common/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/common",
            "tsConfig": "packages/common/tsconfig.lib.json",
            "packageJson": "packages/common/package.json",
            "main": "packages/common/src/index.ts",
            "assets": [
              "packages/common/*.md",
              {
                "input": "./packages/common/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/common",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/common",
                "glob": "builders.json",
                "output": "."
              },
              {
                "input": "./packages/common",
                "glob": "migrations.json",
                "output": "."
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": ["dist/packages/common"],
          "options": {
            "commands": [
              {
                "command": "nx build-base common"
              }
            ],
            "parallel": false
          }
        }
      }
    },
    "cli": {
      "root": "packages/cli",
      "sourceRoot": "packages/cli/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/cli/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cli/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/cli",
            "tsConfig": "packages/cli/tsconfig.lib.json",
            "packageJson": "packages/cli/package.json",
            "main": "packages/cli/src/index.ts",
            "assets": ["packages/cli/*.md"]
          }
        }
      }
    },
    "dep-graph": {
      "root": "packages/dep-graph",
      "sourceRoot": "packages/dep-graph/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/packages/dep-graph",
            "index": "packages/dep-graph/src/index.html",
            "main": "packages/dep-graph/src/main.ts",
            "polyfills": "packages/dep-graph/src/polyfills.ts",
            "tsConfig": "packages/dep-graph/tsconfig.app.json",
            "assets": [
              "packages/dep-graph/src/favicon.ico",
              "packages/dep-graph/src/assets"
            ],
            "styles": ["packages/dep-graph/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/dep-graph/src/main.ts",
                  "with": "packages/dep-graph/src/main.release.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "dep-graph:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "dep-graph:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/dep-graph/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/dep-graph/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dep-graph-e2e": {
      "root": "packages/dep-graph-e2e",
      "sourceRoot": "packages/dep-graph-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/dep-graph-e2e/cypress.json",
            "tsConfig": "packages/dep-graph-e2e/tsconfig.e2e.json",
            "devServerTarget": "dep-graph:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dep-graph:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/dep-graph-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "typescript": {
      "root": "packages/typescript",
      "sourceRoot": "packages/typescript/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/typescript/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/typescript/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/typescript",
            "tsConfig": "packages/typescript/tsconfig.lib.json",
            "packageJson": "packages/typescript/package.json",
            "main": "packages/typescript/src/index.ts",
            "assets": [
              "packages/typescript/*.md",
              {
                "input": "./packages/typescript/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/typescript",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/typescript",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": ["dist/packages/typescript"],
          "options": {
            "commands": [
              {
                "command": "nx build-base typescript"
              }
            ],
            "parallel": false
          }
        }
      }
    },
    "typescript-e2e": {
      "projectType": "application",
      "root": "packages/typescript-e2e",
      "sourceRoot": "packages/typescript-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "typescript:build",
            "npmPackageName": "@sinbix/typescript",
            "pluginOutputPath": "dist/packages/typescript",
            "jestConfig": "packages/typescript-e2e/jest.config.js"
          }
        }
      }
    },
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/core/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/src/index.ts",
            "assets": ["packages/core/*.md"]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": ["dist/packages/core"],
          "options": {
            "commands": [
              {
                "command": "nx build-base core"
              },
              {
                "command": "nx build dep-graph --configuration production"
              },
              {
                "command": "node ./tools/scripts/copy-dep-graph.js"
              }
            ],
            "parallel": false
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "dep-graph"
}
