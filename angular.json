    "plugins-common": {
      "root": "packages/plugins/common",
      "sourceRoot": "packages/plugins/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/plugins/common/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/plugins/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-base": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/plugins/common",
            "tsConfig": "packages/plugins/common/tsconfig.lib.json",
            "packageJson": "packages/plugins/common/package.json",
            "main": "packages/plugins/common/src/index.ts",
            "assets": [
              "packages/plugins/common/*.md",
              {
                "input": "./packages/plugins/common/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/plugins/common",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/plugins/common",
                "glob": "builders.json",
                "output": "."
              },
              {
                "input": "./packages/plugins/common",
                "glob": "migrations.json",
                "output": "."
              }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages/plugins/common"
          ],
          "options": {
            "commands": [
              {
                "command": "nx build-base plugins-common"
              }
            ],
            "parallel": false
          }
        }
      }
    },
