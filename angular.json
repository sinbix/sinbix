    "apps-dep-graph": {
      "root": "packages/apps/dep-graph",
      "sourceRoot": "packages/apps/dep-graph/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/packages/apps/dep-graph",
            "index": "packages/apps/dep-graph/src/index.html",
            "main": "packages/apps/dep-graph/src/main.ts",
            "polyfills": "packages/apps/dep-graph/src/polyfills.ts",
            "tsConfig": "packages/apps/dep-graph/tsconfig.app.json",
            "assets": [
              "packages/apps/dep-graph/src/favicon.ico",
              "packages/apps/dep-graph/src/assets"
            ],
            "styles": [
              "packages/apps/dep-graph/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/apps/dep-graph/src/main.ts",
                  "with": "packages/apps/dep-graph/src/main.release.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "apps-dep-graph:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "apps-dep-graph:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/apps/dep-graph/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/apps/dep-graph/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
