{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom",
      "esnext.asynciterable"
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@sinbix/common": [
        "plugins/common/src/index.ts"
      ],
      "@sinbix/cli": [
        "sinbix/cli/src/index.ts"
      ],
      "@sinbix/cli/*": [
        "sinbix/cli/*"
      ],
      "@sinbix/core": [
        "sinbix/core/src/index.ts"
      ],
      "@sinbix/core/*": [
        "sinbix/core/*"
      ],
      "@sinbix/utils": [
        "sinbix/utils/src/index.ts"
      ],
      "@sinbix/node": [
        "plugins/node/src/index.ts"
      ],
      "@sinbix/plugin/*": [
        "plugins/plugin/*"
      ],
      "@sinbix/sinbix/sinbix": [
        "sinbix/sinbix/src/index.ts"
      ],
      "@sinbix/web": [
        "packages/plugins/web/src/index.ts"
      ],
      "@sinbix/nest": [
        "plugins/nest/src/index.ts"
      ],
      "@sinbix/angular": [
        "plugins/angular/src/index.ts"
      ],
      "@sinbix/xplat": [
        "plugins/xplat/src/index.ts"
      ],
      "@sinbix/demo/deploy": [
        "packages/demo/deploy/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server/ui": [
        "packages/demo/apps/nest/server/ui/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server/services": [
        "packages/demo/apps/nest/server/services/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server/utils": [
        "packages/demo/apps/nest/server/utils/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server/public": [
        "packages/demo/apps/nest/server/public/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server-blog-ms/ui": [
        "packages/demo/apps/nest/server-blog-ms/ui/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server-blog-ms/services": [
        "packages/demo/apps/nest/server-blog-ms/services/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server-blog-ms/db": [
        "packages/demo/apps/nest/server-blog-ms/db/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server-blog-ms/utils": [
        "packages/demo/apps/nest/server-blog-ms/utils/src/index.ts"
      ],
      "@sinbix/demo/apps/nest/server-blog-ms/public": [
        "packages/demo/apps/nest/server-blog-ms/public/src/index.ts"
      ],
      "@sinbix-nest/microservices": [
        "packages/nest/microservices/src/index.ts"
      ],
      "@sinbix-common/utils": [
        "packages/common/utils/src/index.ts"
      ],
      "@sinbix-common/validator": [
        "packages/common/validator/src/index.ts"
      ],
      "@sinbix-angular/common": [
        "packages/angular/common/index.ts"
      ],
      "@sinbix-angular/common/*": [
        "packages/angular/common/*"
      ],
      "@sinbix-angular/material/*": [
        "packages/angular/material/*"
      ],
      "@sinbix-nest/common": [
        "packages/nest/common/src/index.ts"
      ],
      "@sinbix/sinbix/ng/data-access": [
        "sinbix/ng/data-access/index.ts"
      ],
      "@sinbix/sinbix/ng/features/*": [
        "sinbix/ng/features/*"
      ],
      "@sinbix/sinbix/ng/ui/*": [
        "sinbix/ng/ui/*"
      ],
      "@sinbix/sinbix/ng/utils/*": [
        "sinbix/ng/utils/*"
      ],
      "@sinbix/demo/ng/data-access": [
        "demo/ng/data-access/src/index.ts"
      ],
      "@sinbix/demo/ng/features": [
        "demo/ng/features/src/index.ts"
      ],
      "@sinbix/demo/ng/ui": [
        "demo/ng/ui/src/index.ts"
      ],
      "@sinbix/demo/ng/utils": [
        "demo/ng/utils/src/index.ts"
      ],
      "@sinbix/demo/ng/public": [
        "demo/ng/public/src/index.ts"
      ],
      "@sinbix/demo/shared/types": [
        "demo/shared/types/src/index.ts"
      ],
      "@sinbix/demo/nest/db": [
        "demo/nest/db/src/index.ts"
      ],
      "@sinbix/demo/nest/public": [
        "demo/nest/public/src/index.ts"
      ],
      "@sinbix/demo/nest/services": [
        "demo/nest/services/src/index.ts"
      ],
      "@sinbix/demo/nest/ui": [
        "demo/nest/ui/src/index.ts"
      ],
      "@sinbix/demo/nest/utils": [
        "demo/nest/utils/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
