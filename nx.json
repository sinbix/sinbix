{
  "npmScope": "sinbix",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@sinbix/core/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build-base",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "sinbix": {
      "tags": [],
      "implicitDependencies": [
        "cli"
      ]
    },
    "common-e2e": {
      "tags": [],
      "implicitDependencies": [
        "common"
      ]
    },
    "common": {
      "tags": [],
      "implicitDependencies": [
        "core"
      ]
    },
    "cli": {
      "implicitDependencies": [
        "common"
      ]
    },
    "dep-graph": {
      "tags": []
    },
    "dep-graph-e2e": {
      "tags": [],
      "implicitDependencies": [
        "core",
        "dep-graph"
      ]
    },
    "core": {
      "tags": []
    },
    "node-e2e": {
      "tags": [],
      "implicitDependencies": [
        "common",
        "node"
      ]
    },
    "node": {
      "tags": [],
      "implicitDependencies": [
        "common"
      ]
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  }
}
