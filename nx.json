{
  "npmScope": "sinbix",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@sinbix/core/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "build-base", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "sinbix": {
      "tags": []
    },
    "common": {
      "tags": [],
      "implicitDependencies": ["core"]
    },
    "cli": {
      "implicitDependencies": ["common"]
    },
    "dep-graph": {
      "tags": []
    },
    "dep-graph-e2e": {
      "tags": [],
      "implicitDependencies": ["dep-graph"]
    },
    "typescript": {
      "tags": [],
      "implicitDependencies": ["common"]
    },
    "typescript-e2e": {
      "tags": [],
      "implicitDependencies": ["typescript"]
    },
    "core": {
      "tags": []
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  }
}
